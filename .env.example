OPENAI_API_KEY=***
E2B_API_KEY=***
MODEL="openai/gpt-5-nano"

# Github App Configuration (Docs Bot)
# See docs/docs_handler/infra/policy_system_implementation_plan.md for setup instructions

# GitHub App ID (numeric ID from app settings, e.g., 123456)
GITHUB_APP_ID=

# GitHub App Installation ID (from installation URL, e.g., 107965932)
GITHUB_APP_INSTALLATION_ID=

# Base64-encoded private key from GitHub App
# Generate: base64 -i downloaded-key.pem | tr -d '\n' (Linux/macOS)
# Or: [Convert]::ToBase64String([System.IO.File]::ReadAllBytes("key.pem")) (PowerShell)
GITHUB_APP_PRIVATE_KEY=

# Webhook secret for signature verification (set in GitHub App settings)
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# Personal access token for installing custom packages
GITHUB_PAT=your_personal_access_token_here

#Supabase
SUPABASE_URL=http://
SUPABASE_PUBLISHABLE_KEY=your_publishable_key_here
SUPABASE_SECRET_KEY=your_secret_key_here
SUPABASE_SCHEMA=my_schema_name
SUPABASE_CONNECTION_STRING=postgresql://postgres:postgres@127.0.0.1:54322/postgres

AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_CLIENT_CERTIFICATE=
AZURE_TENANT=