{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://winston-ai-mastra.vercel.app/api/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "https://winston-ai-mastra.vercel.app/auth/$1"
    },
    {
      "source": "/winston/(.*)",
      "destination": "https://winston-ai-mastra.vercel.app/winston/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
